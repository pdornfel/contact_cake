<h1>Contacts</h1>

<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User Id</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.user_id %></td>
        <td><%= link_to "Display", contact %></td>
      </tr>
      <% end %>
  </tbody>
</table>
<br>
<%= link_to "New Contact", new_contact_path %>